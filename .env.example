# TechnonextPay Environment Configuration
# Copy this file to .env and fill in your actual credentials

# Merchant Account Credentials
# Get these from your TechnonextPay merchant dashboard
USERNAME=your_merchant_username_here
PASSWORD=your_merchant_password_here
SIGNATURE=your_merchant_signature_here
MERCHANT_CODE=your_merchant_code_here

# API Configuration
# Use sandbox for testing: https://sandbox.technonextpay.com
# Use production for live payments: https://api.technonextpay.com
TechnonextPay_API_ENDPOINT=https://sandbox.technonextpay.com

# Callback URLs
# These are the URLs where customers will be redirected after payment
SUCCESS_URL=https://yourdomain.com/success.php
FAILURE_URL=https://yourdomain.com/failure.php
CANCEL_URL=https://yourdomain.com/cancel.php

# IPN (Instant Payment Notification) URL
# This URL receives asynchronous payment notifications from TechnonextPay
IPN_URL=https://yourdomain.com/ipn.php

# Logging Configuration
# Directory where payment logs will be stored
LOG_LOCATION=./logs/technonextpay

# SSL Security Settings
# Set to 1 for production (recommended), 0 for testing only
CURLOPT_SSL_VERIFYPEER=1

# Optional: Order Prefix
# Prefix for generated order IDs (leave empty for default)
ORDER_PREFIX=PAY_</content>
